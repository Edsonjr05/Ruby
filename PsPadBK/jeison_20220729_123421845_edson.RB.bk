
array =[]
puts '--Leitura do Json--'

puts 'Informe o caminho do arquivo'
    arquivo = gets.chomp.to_s

file = File.open(arquivo)

 file.each do |line|
      tela = line.match(/SR\w+\_\w+/)
      if tela == nil || !array.include?(tela)
      else
        array.append(tela)
      end

end

puts array





